CASE when ya.rowid = '536870914' then 12.534742328274342 else ya.rowid end as Lat,CASE when ya.rowid = '536870914' then -70.01948666675185 else ya.rowid end as Long,CASE when ya.rowid = '536870914' then 'MN SP FCCA'else ya.rowid end as Name, CASE when bp1.OutPercentUtil >= 80 and ya.status < 10 and yr.status < 5 or bp1.inPercentUtil >= 80 and ya.status < 10 and yr.status < 5 then 'Bandwidth' when yr.status >= 5 and ya.status < 10 then 'Ranged' when ya.status >= 10 and ya.status < 20  then 'Warning' when ya.status >= 20 then 'Offline' else 'Online' end as Marker FROM (SELECT RowID, CustomPollerAssignmentID, Status, case  when RowID = 536870914 then 'SERO PATRISHI FCCA COMPLEX' end as portname FROM orion.npm.CustomPollerStatus WHERE RowID = '536870914' ) ya INNER JOIN (SELECT RowID,CustomPollerAssignmentID, Status FROM orion.npm.CustomPollerStatus WHERE RowID = '536870914') yr  ON yr.rowid= '536870914' INNER JOIN (SELECT OutPercentUtil, inPercentUtil, interfacealias FROM orion.npm.Interfaces) bp1 on ya.portname = bp1.interfacealias where ya.CustomPollerAssignmentID = '300db92e-ddd7-4981-8934-09fd666c3efb' and yr.CustomPollerAssignmentID = 'a4ee39ad-aec6-4801-8618-262538359e23'
